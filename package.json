{
  "name": "accessifyhtml5",
  "version": "2.0.0",
  "description": "While most browsers work all right with the new semantic elements of HTML5, they don't add the ARIA accessibility attributes that the specification demands. This small JavaScript adds those attributes to enhance accessibility of web sites.",
  "main": "accessifyhtml5.js",
  "browser": "accessifyhtml5.js",
  "devDependencies": {
    "semistandard": "^12.0.1"
  },
  "peerDependencies": {
    "buster-test": "^0.6.2"
  },
  "repository": "https://github.com/yatil/accessifyhtml5.js.git",
  "x-repo-1": "https://github.com/nfreear/accessifyhtml5.js.git",
  "x-repo-2": "https://github.com/BEllis/accessifyhtml5.js.git",
  "bugs": "https://github.com/yatil/accessifyhtml5.js/issues",
  "author": "Eric Eggert {added to NPM by Ben Ellis}",
  "contributors": [
    {
      "name": "Many contributors ... see README",
      "url": "https://github.com/yatil/accessifyhtml5.js#acknowledgements"
    }
  ],
  "license": "MIT",
  "keywords": [
    "accessibility",
    "aria",
    "html5",
    "wai-aria"
  ],
  "scripts": {
    "test": "eslint .",
    "semi": "semistandard  # Not in use.",
    "fix": "# semistandard --fix  # Not in use."
  },
  "eslintConfig": {
    "extends": "semistandard",
    "rules": {
      "camelcase": "warn"
    }
  },
  "eslintIgnore": [ "*.min.js" ],
  "semistandard": {
    "exclude": [
      "X*.jquery.*"
    ]
  },
  "files": [
    "accessifyhtml5.jquery.js"
  ]
}
